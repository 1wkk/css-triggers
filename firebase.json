{
  "firebase": "css-triggers",
  "public": "dist",
  "ignore": [
    "firebase.json",
    "**/.*",
    "**/node_modules/**",
    "**/_suite"
  ],

  "headers": [{
    "source": "**/",
    "headers": [{
      "key": "Cache-Control",
      "value": "max-age=0, no-cache"
    }]
  },{
    "source": "**/**",
    "headers": [{
      "key": "Cache-Control",
      "value": "max-age=0, no-cache"
    }]
  },{
    "source": "**/*.html",
    "headers": [{
      "key": "Cache-Control",
      "value": "max-age=0, no-cache"
    }]
  },{
    "source": "**/*.js",
    "headers": [{
      "key": "Cache-Control",
      "value": "max-age=31536000"
    }]
  },{
    "source": "**/*.css",
    "headers": [{
      "key": "Cache-Control",
      "value": "max-age=0, no-cache"
    }]
  }],

